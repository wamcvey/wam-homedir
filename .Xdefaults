	Shift<Key>Left:		scroll-back(1,line)	\n\
	Shift<Key>Right:	scroll-forw(1,line)	\n\
	Shift<Key>Up:		scroll-back(1,page)	\n\
	Shift<Key>Down:		scroll-forw(1,page)
